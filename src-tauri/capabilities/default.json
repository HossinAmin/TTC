{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-execute",
    "shell:default",
    "core:app:allow-default-window-icon",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "core:path:allow-resolve",
    "fs:allow-resource-read-recursive"
  ]
}
